/****************************************************************
	CHANGE_ITEM
	Any change? (Input screen fields)
****************************************************************/

CHANGE_ITEM:
	procedure;

declare		SELECT               fixed binary (15);
call ANY_CHANGE (1, 93,SELECT,RETURN_CODE);
if (RETURN_CODE = END_TYPED) ! (RETURN_CODE = ESCAPE) then
   do;
   ERROR_INPUT_SCREEN = YES;
   do;
   END_CHANGE_ITEM = YES;
   return;
   end;
   end;
if SELECT = 0 then
   do;
   END_CHANGE_ITEM = YES;
   return;
   end;
if SELECT >=  1 then if SELECT <=  3 then
   do;

      LOOP_INDEX = SELECT +   9;
   call INPUT_FIELD (LOOP_INDEX,INPUT_LENGTH,RETURN_CODE);
   end;
if SELECT =  4 then
   do;
   do LOOP_INDEX =  13 to  14;
      call INPUT_FIELD (LOOP_INDEX,INPUT_LENGTH,RETURN_CODE);
      if (RETURN_CODE = END_TYPED) ! (RETURN_CODE = ESCAPE) then
         do;
         ERROR_INPUT_SCREEN = YES;
         return;
         end;
   end;
   LOOP_INDEX = LOOP_INDEX - 1;
   end;
if SELECT >=  5 then if SELECT <=  7 then
   do;
   if SELECT <=  6 then
      LOOP_INDEX = SELECT +  11;
   if SELECT =  7 then
      LOOP_INDEX = 19;
   if LOOP_INDEX =  17 then
      do;

      /* CHECK_GL_HDR - Don't allow changes if this is a tour */

      if AP_OPEN_TOUR ^= 0 then
         LOOP_INDEX = 99;

      end;
   if LOOP_INDEX =  19 then
      do;

      /* CHECK_GL_HDR - Don't allow changes if this is a tour */

      if AP_OPEN_TOUR ^= 0 then
         LOOP_INDEX = 99;

      end;
   call INPUT_FIELD (LOOP_INDEX,INPUT_LENGTH,RETURN_CODE);
   if LOOP_INDEX =  16 then
      do;

      /* CHECK_INV_SIGN - Check sign of amount */

      if AP_VEN_INV_AMT < 0 then
         do;
         if AP_INV_NEW = "N" then
            do;
            if AP_ENTRY_TYPE ^= CR_INV then
               do;
               call DISPLAY_MESSAGE("<BEL>Invoice amount must be positive"c);
               call PRESS_RETURN;
               LOOP_INDEX = LOOP_INDEX - 1;
               end;
            end;
         else
            do;
            call OUTPUT_LINE(23,10,"Are you sure you want to enter a credit invoice?");
            call INPUT_DATA(23,61,1,"Y",byteaddr(ANSWER),INPUT_LENGTH,RETURN_CODE);
            call CLEAR_TO_BOS(23);
            call DUMP_BUFFER;
            if ANSWER ^= "Y" then
               LOOP_INDEX = LOOP_INDEX -1;
            end;
         end;

      end;
   if LOOP_INDEX =  17 then
      do;

      /* READ_SECOND_5 - Read second */

      GL_ACCOUNT_NO = AP_OVER_UNDER_GL;
      call READ_KEYED (GL_CHART_NO,KEY_AREA_1,ERROR_CODE);
      if ERROR_CODE = RECORD_NOT_FOUND then
         do;
         call DISPLAY_MESSAGE ("Record not found!!!<BEL>"c);
         call PRESS_RETURN;
         call CLEAR_MESSAGE;
         RETURN_CODE = DO_AGAIN;
         LOOP_INDEX = LOOP_INDEX - 1;
         end;
      else
         do;
         do LOOP_INDEX =  18 to  18;
            call OUTPUT_FIELD (LOOP_INDEX);
         end;
         LOOP_INDEX = LOOP_INDEX - 1;
         end;
      end;
   if LOOP_INDEX =  19 then
      do;

      /* READ_SECOND_6 - Read secondary file */

      GL_ACCOUNT_NO = AP_OPEN_AP_ACCT;
      call READ_KEYED (GL_CHART_NO,KEY_AREA_1,ERROR_CODE);
      if ERROR_CODE = RECORD_NOT_FOUND then
         do;
         call DISPLAY_MESSAGE ("Record not found!!!<BEL>"c);
         call PRESS_RETURN;
         call CLEAR_MESSAGE;
         RETURN_CODE = DO_AGAIN;
         LOOP_INDEX = LOOP_INDEX - 1;
         end;
      else
         do;
         do LOOP_INDEX =  20 to  20;
            call OUTPUT_FIELD (LOOP_INDEX);
         end;
         LOOP_INDEX = LOOP_INDEX - 1;
         end;
      end;
   end;
if SELECT >= 92 then if SELECT <= 92 then
   do;
   if SELECT = 92 then
      LOOP_INDEX =  1;
   if LOOP_INDEX =   1 then
      call DISPLAY_MSG_NO ( 54);
   call INPUT_FIELD (LOOP_INDEX,INPUT_LENGTH,RETURN_CODE);
   end;
if SELECT = 93 then
   do;
   do LOOP_INDEX =   2 to   3;
      call INPUT_FIELD (LOOP_INDEX,INPUT_LENGTH,RETURN_CODE);
      if (RETURN_CODE = END_TYPED) ! (RETURN_CODE = ESCAPE) then
         do;
         ERROR_INPUT_SCREEN = YES;
         return;
         end;
   end;
   LOOP_INDEX = LOOP_INDEX - 1;
   end;
return;
end;	/* CHANGE_ITEM          */

