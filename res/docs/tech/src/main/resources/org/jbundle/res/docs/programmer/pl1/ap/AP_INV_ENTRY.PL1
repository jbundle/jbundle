      END_ENTER_AP_VEN = NO;
      do while (END_ENTER_AP_VEN ^= YES);
         call ENTER_AP_VEN;
      end;
      end;
   else
   do;

      /* SET_AP_VEN - Set the vendor number to the default */

      AP_OPEN_VENDOR = AP_INV_DEF_VEN;


      /* SET_AP_CURR - Set A/P currency */

      AP_OPEN_CURR = AP_VEN_CURR;

      call DISPLAY_DATA_H;
   end;


   /* RETURN_IF_INP_ERR - Return if input error */

   if ERROR_INPUT_SCREEN then
      do;
      END_ENTER_NEW_INVOIC = YES;
      return;
      end;

   END_ENTER_AP_TR = NO;
   do while (END_ENTER_AP_TR ^= YES);
      call ENTER_AP_TR;
   end;

   /* RETURN_IF_INP_ERR - Return if input error */

   if ERROR_INPUT_SCREEN then
      do;
      END_ENTER_NEW_INVOIC = YES;
      return;
      end;


   /* INIT_AP_DATA - Init data for any invoice */

   if AP_OPEN_TOUR_GL = 0 then
      AP_OPEN_TOUR_GL = HDR_LAND_GL;
   if AP_OPEN_TOUR_GL = 0 then
      AP_OPEN_TOUR_GL = AP_DEF_TOUR_GL;
   if AP_EST_GL = 0 then
      AP_EST_GL = HDR_UNINV_GL;
   if AP_EST_GL = 0 then
      AP_EST_GL = AP_DEF_EST;
   if AP_OVER_UNDER_GL = 0 then
      AP_OVER_UNDER_GL = HDR_COST_OU_GL;
   if AP_OVER_UNDER_GL = 0 then
      AP_OVER_UNDER_GL = AP_DEF_OU;
   if AP_OPEN_AP_ACCT = 0 then
      AP_OPEN_AP_ACCT = HDR_AP_GL;
   if AP_OPEN_AP_ACCT = 0 then
      AP_OPEN_AP_ACCT = AP_DEF_AP;
   if AP_GAIN_LOSS_GL = 0 then
      AP_GAIN_LOSS_GL = HDR_CURR_OU_GL;
   if AP_GAIN_LOSS_GL = 0 then
      AP_GAIN_LOSS_GL = AP_DEF_GL;


   /* SET_AP_INVOICE - Set the ivoice up */

   AP_SERVICES = "";
   AP_DEP_EST = 0;
   AP_START_SVC = TOUR_DEP_DATE;
   AP_END_SVC = TOUR_DEP_DATE;

   call ENTER_DATA_G;

   /* RETURN_IF_INP_ERR - Return if input error */

   if ERROR_INPUT_SCREEN then
      do;
      END_ENTER_NEW_INVOIC = YES;
      return;
      end;


   /* SET_DEF_VEN_INV - Set the vendor default info. */

   if AP_INV_DEF_VEN ^= "" then
      if AP_VEN_INV_NO = "" then
      if AP_VEN_INV_DATE = 0 then
         do;
         AP_VEN_INV_NO = AP_INV_DEF_NO;
         AP_VEN_INV_DATE = AP_INV_DEF_DATE;
         end;

   call ENTER_DATA_I;

   /* RETURN_IF_INP_ERR - Return if input error */

   if ERROR_INPUT_SCREEN then
      do;
      END_ENTER_NEW_INVOIC = YES;
      return;
      end;


   /* CHECK_ENT_NTGL - If non-tour, enter G/L number(s) */

   if AP_OPEN_TOUR = 0 then
      do;

      /* ENTER_DATA_K - Enter data K */

      do LOOP_INDEX =  17 to  17;
         if LOOP_INDEX =  17 then
            do;

            /* CHECK_GL_HDR - Don't allow changes if this is a tour */

            if AP_OPEN_TOUR ^= 0 then
               LOOP_INDEX = 99;

            end;
         call INPUT_FIELD (LOOP_INDEX,INPUT_LENGTH,RETURN_CODE);
         if (RETURN_CODE = END_TYPED) ! (RETURN_CODE = ESCAPE) then
            do;
            ERROR_INPUT_SCREEN = YES;
            return;
            end;
         if LOOP_INDEX =  17 then
            do;

            /* READ_SECOND_5 - Read second */

            GL_ACCOUNT_NO = AP_OVER_UNDER_GL;
            call READ_KEYED (GL_CHART_NO,KEY_AREA_1,ERROR_CODE);
            if ERROR_CODE = RECORD_NOT_FOUND then
               do;
               call DISPLAY_MESSAGE ("Record not found!!!<BEL>"c);
               call PRESS_RETURN;
               call CLEAR_MESSAGE;
               RETURN_CODE = DO_AGAIN;
               LOOP_INDEX = LOOP_INDEX - 1;
               end;
            else
               do;
               do LOOP_INDEX =  18 to  18;
                  call OUTPUT_FIELD (LOOP_INDEX);
               end;
               LOOP_INDEX = LOOP_INDEX - 1;
               end;
            end;
      end;
      LOOP_INDEX = LOOP_INDEX - 1;
      do LOOP_INDEX =  19 to  19;
         call OUTPUT_FIELD (LOOP_INDEX);
         if LOOP_INDEX =  19 then
            do;

            /* READ_SECOND_6 - Read secondary file */

            GL_ACCOUNT_NO = AP_OPEN_AP_ACCT;
            call READ_KEYED (GL_CHART_NO,KEY_AREA_1,ERROR_CODE);
            if ERROR_CODE = RECORD_NOT_FOUND then
               do;
               GL_ACCOUNT_DESC = "** RECORD NOT FOUND **";
               end;
            else
               do;
               do LOOP_INDEX =  20 to  20;
                  call OUTPUT_FIELD (LOOP_INDEX);
               end;
               LOOP_INDEX = LOOP_INDEX - 1;
               end;
            end;
      end;
      LOOP_INDEX = LOOP_INDEX - 1;
      end;
   else
      do;
      call DISPLAY_DATA_K;
      end;


   /* RETURN - Normal Return */

   do;
   END_ENTER_NEW_INVOIC = YES;
   return;
   end;

return;
end;	/* ENTER_NEW_INVOICE    */

