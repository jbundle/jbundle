   /* RESET_DEF_VENDOR - Reset the default vendor */

   if AP_INV_DEF_VEN ^= "" then
      do;

      /* REDISPLAY_SCREEN - Re-display the screen */

      if NOT_FIRST_TIME then
         call DISPLAY_SCREEN (AP_INV_ENTRY_1);
      NOT_FIRST_TIME = YES;
      call DISPLAY_DATA_D;
      end;
   AP_INV_DEF_VEN = "";
   AP_INV_DEF_NO = "";
   AP_INV_DEF_DATE = 0;
   LINE = 0;


   /* CLEAR_DATA_D - Clear fields */

   do LOOP_INDEX =   1 to   1;
      call CLEAR_FIELD(LOOP_INDEX);
   end;
   call ENTER_DATA_A;

   /* RETURN_IF_END - Return if end typed */

   if (RETURN_CODE = END_TYPED) ! (RETURN_CODE = ESCAPE) then
      do;
      END_ENTER_VENDOR_INV = YES;
      return;
      end;


   /* SETUP_KEY_3 - Set up the key */

   AP_VEN_NO = AP_OPEN_VENDOR;
   KEY_NO = 1;

   /* READ_RECORD_3 - Read the record */

   call READ_KEYED (AP_VENDOR_NO,KEY_NO,ERROR_AP_VENDOR);
   if ERROR_AP_VENDOR > 3 then
      call PRINT_ERROR (AP_VENDOR_NO,ERROR_AP_VENDOR);

   /* AP_VENDOR_FOUND - Found vendor code? */

   ERROR_INPUT_SCREEN = NO;
   if ERROR_AP_VENDOR = NORMAL_RETURN then
      do;
      call FOUND_AP_VENDOR;
      end;
   else
      do;

      /* RECORD_NOT_FOUND - Record not found */

      call DISPLAY_MESSAGE("Record not found!!!<BEL>"c);
      call PRESS_RETURN;
      ERROR_INPUT_SCREEN = YES;

      if ERROR_INPUT_SCREEN then return;
      end;

   /* RETURN - Normal Return */

   do;
   END_ENTER_VENDOR_INV = YES;
   return;
   end;

return;
end;	/* ENTER_VENDOR_INV     */

