   call CLEAR_FIELD(LOOP_INDEX);
end;
return;
end;	/* CLEAR_DATA_L         */

/****************************************************************
	GET_DEPART_EST
	Get the departure estimate to insert inv
****************************************************************/

GET_DEPART_EST:
	procedure;


   /* RESET_INPUT_ERROR - Set input error to NO */

   ERROR_INPUT_SCREEN = NO;


   /* RESET_VENDOR_ENTER - Reset flags */

   AP_INV_VAGAIN = "N";
   AP_INV_NEW = "N";

   call ENTER_DATA_D;

   /* RETURN_IF_END - Return if end typed */

   if (RETURN_CODE = END_TYPED) ! (RETURN_CODE = ESCAPE) then
      do;
      END_GET_DEPART_EST = YES;
      return;
      end;


   /* CHECK_ENT_NUM - Check to see what the entered */

   if AP_ENTRY_NO ^= 0 then
      do;
      END_READ_OLD_INVOICE = NO;
      do while (END_READ_OLD_INVOICE ^= YES);
         call READ_OLD_INVOICE;
      end;
      end;
   else
      do;
      if INPUT_LENGTH = 0 then
         do;
         END_ENTER_VENDOR_INV = NO;
         do while (END_ENTER_VENDOR_INV ^= YES);
            call ENTER_VENDOR_INV;
         end;
         end;
      else
         do;
         END_ENTER_NEW_INVOIC = NO;
         do while (END_ENTER_NEW_INVOIC ^= YES);
            call ENTER_NEW_INVOICE;
         end;
         if (AP_INV_VAGAIN = "Y") ! (ERROR_INPUT_SCREEN = YES) then
            do;
            AP_NEXT_ENTRY = AP_ENTRY_NO;

            /* DELETE_RECORD - Delete the record */

            call DELETE_RECORD (AP_OPEN_NO,ERROR_CODE);
            end;
         end;
      end;


   /* CHECK_INV_ENT - Enter again? */

   if AP_INV_VAGAIN = "Y" then
      return;


   /* RETURN_IF_INP_ERR - Return if input error */

   if ERROR_INPUT_SCREEN then
      do;
      END_GET_DEPART_EST = YES;
      return;
      end;


   /* RETURN - Normal Return */

   do;
   END_GET_DEPART_EST = YES;
   return;
   end;

return;
end;	/* GET_DEPART_EST       */

/****************************************************************
	ENTER_DATA_D
	Enter screen group D
****************************************************************/

ENTER_DATA_D:
	procedure;

do LOOP_INDEX =   1 to   1;
   if LOOP_INDEX =   1 then
      call DISPLAY_MSG_NO ( 54);
   call INPUT_FIELD (LOOP_INDEX,INPUT_LENGTH,RETURN_CODE);
   if (RETURN_CODE = END_TYPED) ! (RETURN_CODE = ESCAPE) then
      do;
      ERROR_INPUT_SCREEN = YES;
      return;
      end;
end;
LOOP_INDEX = LOOP_INDEX - 1;
return;
end;	/* ENTER_DATA_D         */

/****************************************************************
	READ_OLD_INVOICE
	A/P open found, enter/display info
****************************************************************/

READ_OLD_INVOICE:
	procedure;

declare		ERROR_AP_OPEN        fixed binary (15);

   /* READ_RECORD - Read the record */

   call READ_KEYED (AP_OPEN_NO+?LOCK_RECORD,KEY_AREA_1,ERROR_AP_OPEN);
   if ERROR_AP_OPEN = RECORD_LOCKED then
      do;
      call DISPLAY_MESSAGE("Record locked by " !! LOCK_NAME() !! "<BEL>"c);
      call PRESS_RETURN;
      end;
   if ERROR_AP_OPEN > 3 then
      call PRINT_ERROR (AP_OPEN_NO,ERROR_AP_OPEN);

   /* CHECK_INV_LOCK - Try again if invoice locked */

   if ERROR_AP_OPEN = RECORD_LOCKED then
   do;
      AP_INV_VAGAIN = "Y";
      do;
      END_READ_OLD_INVOICE = YES;
      return;
      end;
      end;

   call DISPLAY_DATA_J;

   /* AP_INVOICE_FOUND - Was the A/P open found? */

   ERROR_INPUT_SCREEN = NO;
   if ERROR_AP_OPEN = NORMAL_RETURN then
      do;
      call CHG_OLD_INVOICE;
      end;
   else
      do;

      /* INVOICE_NOT_FND - Invoice not found */


         /* SET_ENTER_AGAINV - Set flag - enter again */

         AP_INV_VAGAIN = "Y";


         /* MES_NOT_FOUND - Message - record not found!!! */

         call DISPLAY_MESSAGE ("Record not found!!!<Bel>"c);
         call PRESS_RETURN;


         /* RETURN - Normal Return */

         do;
         END_READ_OLD_INVOICE = YES;
         return;
         end;

      if ERROR_INPUT_SCREEN then return;
      end;

   /* RETURN - Normal Return */

   do;
   END_READ_OLD_INVOICE = YES;
   return;
   end;

return;
end;	/* READ_OLD_INVOICE     */

/****************************************************************
	DISPLAY_DATA_J
	Display screen group J
****************************************************************/

DISPLAY_DATA_J:
	procedure;

do LOOP_INDEX =  22 to  22;
   call OUTPUT_FIELD (LOOP_INDEX);
end;
LOOP_INDEX = LOOP_INDEX - 1;
return;
end;	/* DISPLAY_DATA_J       */

/****************************************************************
	CHG_OLD_INVOICE
	Change old invoice
****************************************************************/

CHG_OLD_INVOICE:
	procedure;


   /* CHK_INV_TYPE - Is this a departure est? */

   if AP_ENTRY_TYPE ^= DEP_EST then
      if AP_ENTRY_TYPE ^= CR_INV then if AP_ENTRY_TYPE ^= INVOICE then
         do;
         call DISPLAY_MESSAGE("Invoice amounts can only be entered for departure estimates!!!<BEL>"c);
         call PRESS_RETURN;

         /* SET_ENTER_AGAINV - Set flag - enter again */

         AP_INV_VAGAIN = "Y";

         return;
         end;


   /* CHK_VEN_DEF - Is there a default vendor? */

   if AP_INV_DEF_VEN ^= "" then if AP_INV_DEF_VEN ^= AP_OPEN_VENDOR then
      do;

      /* RESET_DEF_VENDOR - Reset the default vendor */

      if AP_INV_DEF_VEN ^= "" then
         do;

         /* REDISPLAY_SCREEN - Re-display the screen */

         if NOT_FIRST_TIME then
            call DISPLAY_SCREEN (AP_INV_ENTRY_1);
         NOT_FIRST_TIME = YES;
         call DISPLAY_DATA_D;
         end;
      AP_INV_DEF_VEN = "";
      AP_INV_DEF_NO = "";
      AP_INV_DEF_DATE = 0;
      LINE = 0;

      end;


   /* INIT_INV_DATA - Init invoice data */

   AP_OLD_INV_AMT = AP_VEN_INV_AMT;
   if AP_OPEN_AP_ACCT = 0 then
      AP_OPEN_AP_ACCT = AP_DEF_AP;
   if AP_OVER_UNDER_GL = 0 then
      AP_OVER_UNDER_GL = AP_DEF_OU;
   if AP_EST_GL = 0 then
      AP_EST_GL = AP_DEF_EST;


   /* READ_SECOND_2 - Read secondary info */

   AP_VEN_NO = AP_OPEN_VENDOR;
   call READ_KEYED (AP_VENDOR_NO,KEY_AREA_1,ERROR_CODE);
   if ERROR_CODE = RECORD_NOT_FOUND then
      do;
      AP_VEN_NAME = "** RECORD NOT FOUND **";
      end;
   call DISPLAY_DATA_A;
   call DISPLAY_DATA_C;

   /* READ_SECOND_3 - Read second */

   TOUR_NO = AP_OPEN_TOUR;
   call READ_KEYED (BK_TOUR_NO,KEY_AREA_1,ERROR_CODE);
   if ERROR_CODE = RECORD_NOT_FOUND then
      do;
      TOUR_DESC = "** RECORD NOT FOUND **";
      end;
   call DISPLAY_DATA_F;

   /* READ_TR_TOUR_HDR - Read the tour header - Given tour rec. */


      /* SET_FIRST_THDR - Set first tour hdr using tour record */

      HDR_TOUR_CODE = TOUR_CODE;
      HDR_END_ON = TOUR_DEP_DATE;
      call SET_POSITION (BK_TOUR_HDR_NO,KEY_AREA_1,ERROR_CODE);

      /* READNEXT_THDR - Read the next one */

      call READ_NEXT (BK_TOUR_HDR_NO,KEY_AREA_1,ERROR_CODE);
      if ERROR_CODE > 3 then
      	call PRINT_ERROR (BK_TOUR_HDR_NO,ERROR_CODE);

      /* CHECK_THDR_OK - Is the header ok? */

      if (TOUR_CODE ^= HDR_TOUR_CODE) !
         (TOUR_DEP_DATE < HDR_START_ON) !
         (TOUR_DEP_DATE > HDR_END_ON) then
            ERROR_CODE = END_OF_FILE;


   /* CHECK_NO_HDR - Was there a tour header? */

   if ERROR_CODE = END_OF_FILE then
      do;
      HDR_LAND_GL = 0;
      HDR_UNINV_GL = 0;
      HDR_COST_OU_GL = 0;
      HDR_AP_GL = 0;
      HDR_CURR_OU_GL = 0;
      end;


   /* READ_SECOND_4 - read secondary file */

   CURR_CODE = AP_OPEN_CURR;
   call READ_KEYED (AD_CURR_NO,KEY_AREA_1,ERROR_CODE);
   if ERROR_CODE = RECORD_NOT_FOUND then
      do;
      CURR_DESC = "** RECORD NOT FOUND **";
      end;
   else
      do;
      do LOOP_INDEX =  21 to  21;
         call OUTPUT_FIELD (LOOP_INDEX);
      end;
      LOOP_INDEX = LOOP_INDEX - 1;
      end;
   call DISPLAY_DATA_E;
   call DISPLAY_DATA_H;
   call DISPLAY_DATA_G;
   call DISPLAY_DATA_K;

   /* SET_DEF_VEN_INV - Set the vendor default info. */

   if AP_INV_DEF_VEN ^= "" then
      if AP_VEN_INV_NO = "" then
      if AP_VEN_INV_DATE = 0 then
         do;
         AP_VEN_INV_NO = AP_INV_DEF_NO;
         AP_VEN_INV_DATE = AP_INV_DEF_DATE;
         end;


   /* SET_DEF_INV - Set the default invoice amount */

   if AP_VEN_INV_AMT = 0 then
      AP_VEN_INV_AMT = AP_DEP_EST;

   call ENTER_DATA_I;

   /* RETURN - Normal Return */

   return;

return;
end;	/* CHG_OLD_INVOICE      */

